<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_synchro_server_form_arc_product" model="ir.ui.view">
    <field name="name">synchro.server.form.arc_product</field>
    <field name="model">synchro.server</field>
    <field name="inherit_id" ref="sirius_synchro.view_synchro_server_form"/>
    <field name="arch" type="xml">
      <notebook position="inside">
        <page name="account" string="Account">
          <group name="initialisation">
            <field name="date_start"/>
            <field name="date_stop"/>
          </group>
          <group name="plan comptable">
            <field name="account_file"/>
            <button name="button_import_account" string="Check Account" type="object"/>
            <button name="intialize_all" string="Initialize All" type="object"/>
          </group>

        </page>
      </notebook>
    </field>
  </record>
</odoo>
