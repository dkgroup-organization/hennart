<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="stock_move_line_operation_tree_view_inherit" model="ir.ui.view">
            <field name="name">stock.move.line.tree.inherit</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_uom_id']"
                       position="after">
                    <field name="weight" string="Weight"/>
                    <field name="weight_manual" string="Weight manual"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>